/*教师课表*/
.teacherListIcon{position:relative;}
.teacherListIcon .teacher_list{position:absolute;top:40px;left:0;border:1px solid #ccc;padding:5px 10px;background:#fff;display:none;z-index:15;}
.teacherListIcon .teacher_list li{height:28px;line-height:28px;border-bottom:1px dashed #ccc;padding:0 10px;width:45px;}
.teacherListIcon .teacher_list li:hover{background:#eee;cursor:pointer;}

.teachIcon{width:36px;height:36px;display:inline-block;background:url(../images/teachetIcon.png) no-repeat center;}
.teachIcon:hover{background:url(../images/teachetIcon_hover.png) no-repeat center;}

.teach_cont{padding:30px 0;}
.teach_cont table{margin:0 auto;}

.teacherTime_table ul{padding:10px 0;}
.teacherTime_table ul li{color:#999;text-decoration:underline;height:2em;line-height:2em;}
.teacherTime_table ul li:hover{cursor:pointer;}

.teacherTime_table ul li span.stu_data{color:#0bceff;position:relative;text-decoration:underline;width:100px;display:inline-block;text-align:center;padding-right:20px;}
.teacherTime_table ul li span.comme,.teacherTime_table ul li span.give{width:16px;height:16px;line-height:16px;display:inline-block;position:absolute;right:10px;top:5px;font-size:12px;}
.teacherTime_table ul li span.comme{border:1px solid #20ba90;color:#20ba90;}
.teacherTime_table ul li span.give{border:1px solid #c00;color:#c00;}

.teacherTime{display:none;}

.cont_box .teacherTime_cont{position:relative;padding:30px 100px;position:relative;}
.arrow_left,.arrow_right{display:inline-block;width:70px;height:110px;position:absolute;top:50%;margin-top:-55px;}
.arrow_left:hover,.arrow_right:hover{cursor:pointer;}
.arrow_left{background:url(../images/arrow_left.png) no-repeat center;left:0;}
.arrow_right{background:url(../images/arrow_right.png) no-repeat center;right:0;}

.star_td{position:relative;}
.teacherTime_table .star_div{position:absolute;right:-100px;top:-40px;font-size:12px;padding:0 10px 0 20px;background:url(../images/star_bg.png) no-repeat center;width:140px;height:118px;display:none;}
.teacherTime_table .star_div ul li{text-decoration:none;}
.teacherTime_table .star_div ul li span{display:inline-block;width:75px;text-align:left;}
.teacherTime_table .star_div ul li img{margin-top:5px;}
.teacherTime_table .star_div ul li span,.teacherTime_table .star_div ul li img{float:left;}
.see_video{color:#16a6e8;text-decoration:underline;}
.see_video:hover{color:#064172;}
/*视频弹出框*/
.video_box{width:1024px;height:798px;background:#fff;position:fixed;left:50%;top:50%;margin:-399px 0 0 -512px;z-index:30;}
.video_close{position:absolute;top:-15px;right:-15px;}
.video_close:hover{cursor:pointer;}
/*课件上传*/

.upload_detail .upload_back,.work_detail .upload_back,.text_detail .upload_back{float:right;background:url(../images/back.png) no-repeat left center;padding-left:25px;display:inline-blockl;}
.upload_detail .upload_back:hover,.work_detail .upload_back:hover,.text_detail .upload_back:hover{background:url(../images/back_hover.png) no-repeat left center;}
.load_box h3{width:90%;}

.done_icon span a.done_s{width:20px;height:20px;display:inline-block;background:url(../images/done_icon02.png) no-repeat left -180px;}
.done_icon span a.done_s:hover{background:url(../images/done_icon02.png) no-repeat right -180px;}

.load{display:none;}

/*教师列表*/
.teacher_box{display:none;}/*分离页面时去掉*/

.done_icon span a.done_t{width:20px;height:20px;display:inline-block;background:url(../images/done_icon02.png) no-repeat left -200px;}
.done_icon span a.done_t:hover{background:url(../images/done_icon02.png) no-repeat right -200px;}


.watch a,.re_change a{color:#6ac3ec;text-decoration:underline;display:inline-block;margin:0 5px;}
.re_change{border:0;}

.done_icon span a.done_u{width:20px;height:20px;display:inline-block;background:url(../images/done_icon02.png) no-repeat left -220px;}
.done_icon span a.done_u:hover{background:url(../images/done_icon02.png) no-repeat right -220px;}

.month{text-align:center;height:32px;line-height:32px;font-size:18px;color:#999;margin-bottom:10px;}
.attendance_btn{height:40px;text-align:center;}
.attendance_btn input{margin-left:20px;margin-right:20px;}

.gray_btn{background:#ccc;}
.gray_btn:hover{cursor:default;}


.teach_mesg .back_p{padding-right:12%;margin-top:20px;}
.teach_mesg .back_p .back{margin:0;float:right;}

div.put_mesag{height:100px;overflow:auto;border:1px solid #ccc;padding:5px;}
.teach_mesg td{padding:10px 0;}

.teachList_h{overflow:hidden;}
.teachList_h span{float:left;}
.teachList_h input{float:right;width:10%;margin:8px 0 0 0;}

.tea_schedulBox{overflow:hidden;margin-bottom:40px;}
.cont_right ul.tea_scheduleUl{float:left;width:13%;margin-top:20px;}
.cont_right table.tea_scheduleTab{float:left;width:77%;}
.cont_right ul.tea_scheduleUl li{height:35px;line-height:35px;}
.cont_right ul.tea_scheduleUl li.li_head{height:43px;}
.cont_right table.tea_scheduleTab td{height:13px;font-size:14px;}

.tip{color:#999;}

/*角色管理*/
.player{overflow:hidden;margin-top:30px;}
.player_left{float:left;width:15%;border:1px solid #ccc;height:600px;padding-bottom:2%;margin-right:5%;}
.player_right{float:left;width:70%;}

.player_left h5{height:42px;line-height:42px;text-align:center;font-size:21px;font-weight:normal;background:#eee;position:relative;}
.player_left h5 .add_player{position:absolute;top:13px;right:13px;}
.player_left h5 .add_player:hover{cursor:pointer;}
.player_left ul{padding:10px 20px 20px;}
.player_left ul li{height:32px;line-height:32px;font-size:16px;background:url(../images/player.png) no-repeat left center;padding-left:35px;margin-bottom:15px;border-bottom:1px dashed #ccc;}
.player_left ul li:hover,.player_left ul li.current{cursor:pointer;background:url(../images/player.png) no-repeat left center #eee;}

.player_edi{font-size:18px;margin:10px 0;}
.player_edi span{display:inline-block;width:99%;overflow:hidden;}
.player_edi span a.dele_group{float:right;}
.player_edi span i,.player_edi span input{float:left;}
.player_edi span a{float:right;background:url(../images/dele_hover.png) no-repeat right center;padding-right:20px;display:inline-block;color:#666;}
.player_edi span i b{font-weight:normal;}
.player_edi span img{margin-left:30px;float:left;margin-top:5px;}
.player_edi span img:hover{cursor:pointer;}

/*等待页*/
.waiting_page{padding-bottom:20px;}
.waiting_page textarea{width:100%;margin-top:20px;}
.waiting_page p{text-align:center;}

/*权限管理*/
.manage ul{padding-left:30px;font-size:16px;}
.manage ul li{line-height:32px;}
.manage ul li input{margin-right:5px;}

.v1{font-size:21px;font-weight:bold;display:block;margin-top:20px;border-bottom:1px solid #ccc;color:#16a6e8;padding-bottom:5px;}
.v2{font-size:18px;}


/*活动页配置*/
.active_tab tbody tr{color:#ccc;}
.active_tab tbody tr.first_tr{color:#666;}

.active_img{width:30%;height:300px;float:right;border:1px solid #ccc;line-height:300px;text-align:center;}

/*报表*/
.chart_box .cont{border-bottom:1px dashed #666;margin-bottom:40px;padding-left:40px;}
.cont_box .chart_tab{padding-left:30px;}
.chart_tab tr{height:65px;font-size:18px;}
.chart_tab span{display:inline-block;width:50px;height:50px;line-height:50px;border:3px solid #97bbcd;border-radius:100%;text-align:center;}
.border_r{border-right:1px solid #ccc;}

/*所有用户*/
.done_icon span a.done_v{width:20px;height:20px;display:inline-block;background:url(../images/done_icon02.png) no-repeat left -240px;}
.done_icon span a.done_v:hover{background:url(../images/done_icon02.png) no-repeat right -240px;}

.player_edi{overflow:hidden;}
.player_edi span.add_user{float:right;text-align:right;width:auto;}

.stu_tab02 textarea{width:80%;padding:2%;}

/*待办记事*/
.todo_box{overflow:hidden;}
.todo_box li{float:left;width:16%;height:150px;line-height:150px;font-size:24px;text-align:center;margin-right:20px;color:#666;position:relative;border:5px solid #CCC;}
.todo_box li span{position:absolute;top:5px;right:5px;width:28px;height:28px;line-height:28px;font-size:21px;display:inline-block;background:#C00;border-radius:28px;color:#fff;}

.todo_box li:hover,.todo_box li.current{color:#fff;border:5px solid #6ac3ed;background:#6ac3ed;cursor:pointer;}
.todo_box li:hover span,.todo_box li.current span{color:#6ac3ed;background:#fff;}

.todo_tab{margin-top:20px;}
.bg_count{background:#6ac3ed;color:#fff;font-size:18px;}
.todo_tab td{padding:0 10px;}

.do_btn a{color:#C00;}

.todo_box_empty li{background:#ccc;color:#fff;}
.todo_box_empty li span{background:#fff;color:#ccc;}

/*排课要求*/
.done_icon span a.done_x{width:20px;height:20px;display:inline-block;background:url(../images/done_icon02.png) no-repeat left -280px;}
.done_icon span a.done_x:hover{background:url(../images/done_icon02.png) no-repeat right -280px;}
.class_require{background:#eee;min-height:80px;padding:10px;}
.class_require_edi{padding:10px;width:100%;}

/*上传视频、音频*/
.done_icon span a.done_y{width:20px;height:20px;display:inline-block;background:url(../images/done_icon02.png) no-repeat left -300px;}
.done_icon span a.done_y:hover{background:url(../images/done_icon02.png) no-repeat right -300px;}

.done_icon span a.done_z{width:20px;height:20px;display:inline-block;background:url(../images/done_icon02.png) no-repeat left -320px;}
.done_icon span a.done_z:hover{background:url(../images/done_icon02.png) no-repeat right -320px;}

.list07 li{line-height:45px;overflow:hidden;position:relative}
.list07 li span{margin-right:15px;float:left}
.list07 li.searched{text-align:center;}
#id_upload_div{float:left}
#process_info{float:left;width:65%}
.processing{display:none;width:100%;height:4px;font-size:2px;background:#ccc;margin:50px;position:absolute;top:22px;right:0;z-index:10;margin:0;}
.processing p{display:inline-block;width:50%;height:4px;font-size:2px;background:#3CF;position:absolute;top:0;left:0;z-index:11;}














